public class Solution {
    public static String read(int n, int []book, int target){ 
        int left=0;
        int right=n-1;
        while(left<right){
            int sum=book[left] + book[right];
            if(sum==target){
                return "YES";
            }else if(sum<target){
                left++;
                right--;
            }
        }
        return "NO";
        
    }
}
